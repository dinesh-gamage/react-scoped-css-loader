{
  "name": "react-scoped-css-loader",
  "version": "1.1.0",
  "description": "A webpack loader to build scoped css for react applications",
  "main": "lib/classNames.js",
  "types": "lib/classNames.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dinesh-gamage/react-scoped-css-loader.git"
  },
  "keywords": [
    "scoped css",
    "scoped styles",
    "react scoped css",
    "react scoped styles",
    "react",
    "scoped",
    "scoped css loader",
    "scoped style loader",
    "react scoped css loader",
    "react scoped style loader"
  ],
  "author": "dinesh-gamage",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dinesh-gamage/react-scoped-css-loader/issues"
  },
  "homepage": "https://github.com/dinesh-gamage/react-scoped-css-loader#readme",
  "bundleDependencies": false,
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.7.15",
    "@types/webpack": "^5.28.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.2"
  }
}
